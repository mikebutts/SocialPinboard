<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @pin.name %>
</p>

<p>
  <strong>Image:</strong>
  <%= @pin.image %>
</p>
<div class="field">
    <%= @pin.image_url if @pin.image? %>
